<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Conductor">
        <!--Popisy pinů u kontaktu-->
        <attr name="ccConductorLabel" format="string" />
        <attr name="ccDescription" format="string" />

        <!--Směr připojení k sousednímu elementu-->
        <attr name="ccToLeftOf" format="reference" />
        <attr name="ccToRightOf" format="reference" />
        <attr name="ccBelow" format="reference" />
        <attr name="ccAbove" format="reference" />

        <!--Start vodiče-->
        <attr name="ccStart" format="enum">
            <enum name="LEFT" value="1"/>
            <enum name="TOP" value="2"/>
            <enum name="RIGHT" value="3"/>
            <enum name="BOTTOM" value="4"/>
        </attr>

        <!--Konec vodiče-->
        <attr name="ccStop" format="enum">
            <enum name="LEFT" value="1"/>
            <enum name="TOP" value="2"/>
            <enum name="RIGHT" value="3"/>
            <enum name="BOTTOM" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="Component">
        <attr name="cLabel" format="string" />
        <attr name="cDescription" format="string" />

        <attr name="cOrientation" format="enum">
            <enum name="VERTICAL" value="1"/>
            <enum name="HORIZONTAL" value="2"/>
        </attr>

        <attr name="cDirection" format="enum">
            <enum name="NONE" value="-1"/>
            <enum name="LEFT_BOTTOM" value="11"/>
            <enum name="LEFT_CENTER" value="12"/>
            <enum name="LEFT_TOP" value="13"/>
            <enum name="TOP" value="2"/>
            <enum name="RIGHT_BOTTOM" value="31"/>
            <enum name="RIGHT_CENTER" value="32"/>
            <enum name="RIGHT_TOP" value="33"/>
            <enum name="BOTTOM" value="4"/>
        </attr>

        <!--Směr připojení k sousednímu elementu-->
        <attr name="cToLeftOf" format="reference" />
        <attr name="cToRightOf" format="reference" />
        <attr name="cBelow" format="reference" />
        <attr name="cAbove" format="reference" />

        <attr name="cCreateStartNode" format="boolean" />
        <attr name="cCreateEndNode" format="boolean" />
    </declare-styleable>

    <!-- xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->

    <declare-styleable name="ComponentContact">
        <!--Popisy pinů u kontaktu-->
        <attr name="cContactLabel_1" format="string" />
        <attr name="cContactLabel_2" format="string" />

        <!--Typ kontaktu-->
        <attr name="cContactType" format="enum">
            <enum name="NORMAL" value="1"/>
            <enum name="IDLE" value="2"/>
            <enum name="BREAKER" value="3"/>
        </attr>
    </declare-styleable>

    <!-- xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->

    <declare-styleable name="ItemStatusDisplay">
        <attr name="cActive" format="boolean" />
        <attr name="cNumber" format="integer" />
        <attr name="cText01" format="string" />
        <attr name="cText02" format="string" />
        <attr name="cText03" format="string" />
    </declare-styleable>

    <!-- xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->

</resources>